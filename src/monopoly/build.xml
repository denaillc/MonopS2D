<?xml version="1.0" encoding="UTF-8"?>
<project basedir="." default="build">
	<path id="build.classpath">
		<pathelement location="classes"/>
	</path>
	<target name="init">
		<mkdir dir="classes"/>
	</target>
	<target name="clean">
		<delete dir="classes"/>
	</target>
	<target depends="init" name="build">
		<javac srcdir="." destdir="classes" source="1.5" target="1.5" fork="yes">
			<classpath refid="build.classpath"/>
			<include name="Diagramme_de_classes_Monopoly/AutreCarreau.java"/>
			<include name="Diagramme_de_classes_Monopoly/Compagnie.java"/>
			<include name="Diagramme_de_classes_Monopoly/ProprieteAConstruire.java"/>
			<include name="Diagramme_de_classes_Monopoly/CouleurPropriete.java"/>
			<include name="Diagramme_de_classes_Monopoly/Controleur.java"/>
			<include name="Diagramme_de_classes_Monopoly/Joueur.java"/>
			<include name="Diagramme_de_classes_Monopoly/Gare.java"/>
			<include name="Diagramme_de_classes_Monopoly/Carreau.java"/>
			<include name="Diagramme_de_classes_Monopoly/Groupe.java"/>
			<include name="Diagramme_de_classes_Monopoly/Utilitaire.java"/>
			<include name="Diagramme_de_packages/IHM.java"/>
		</javac>
	</target>
</project>